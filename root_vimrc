set nocompatible              " be iMproved, required
filetype off                  " required

" set the runtime path to include vim-plug and initialize
set rtp+=~/.vim_runtime
call plug#begin('~/.vim_runtime/bundle')


"" Plugins
" colors
Plug 'Ralph-Wang/tnb'

" filetype
